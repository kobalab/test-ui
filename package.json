{
  "name": "test-ui",
  "version": "0.1.0",
  "description": "電脳麻将のUIのテスト",
  "private": true,
  "scripts": {
    "build:html": "pug -o dist -P src/html/page/",
    "build:css": "stylus src/css/index.styl -o dist/css/test-ui-0.1.0.css",
    "build:js": "webpack --mode development --devtool inline-source-map",
    "build": "npm run build:html && npm run build:css && npm run build:js",
    "release": "npm run build:html && npm run build:css && webpack --mode production",
    "clean": "rm -f dist/*.html dist/js/* dist/css/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kobalab/test-ui.git"
  },
  "keywords": [],
  "author": "Satoshi Kobayashi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kobalab/test-ui/issues"
  },
  "homepage": "https://github.com/kobalab/test-ui#readme",
  "overrides": {
    "pug": "^3.0.3"
  },
  "dependencies": {
    "jquery": "^3.7.1"
  },
  "devDependencies": {
    "pug-cli": "^1.0.0-alpha6",
    "semver": "^7.6.3",
    "stylus": "^0.64.0",
    "webpack-cli": "^5.1.4"
  }
}
